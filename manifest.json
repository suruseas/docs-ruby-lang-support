{
  "manifest_version": 3,
  "name": "るりまサポート",
  "description": "Ruby 3.2 リファレンスマニュアル(るりま)に掲載のサンプルコードを、Ruby Wasmを使って実行させることができます。",
  "version": "1.2.3",
  "icons": {
    "16": "src/images/icon16.png",
    "48": "src/images/icon48.png",
    "128": "src/images/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "dist/content.js"
      ],
      "matches": [
        "https://docs.ruby-lang.org/ja/3.2/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://docs.ruby-lang.org/*"
      ],
      "resources": [
        "node_modules/ruby-head-wasm-wasi/dist/**"
      ]
    }
  ]
}
