{
  "manifest_version": 3,
  "name": "るりまサポート",
  "description": "Ruby 3.2 リファレンスマニュアル(るりま)に掲載のサンプルコードを、Ruby Wasmを使って実行させることができます。",
  "version": "1.2.1",
  "icons": {
    "16": "src/images/icon16.png",
    "48": "src/images/icon48.png",
    "128": "src/images/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "dist/content.js"
      ],
      "matches": [
        "https://docs.ruby-lang.org/ja/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
